<evio_dictionary>
  <bank name="PART" tag="6000" info="HIT based event reconstruction bank">
    <section name="particle" tag="6001"     num="0"   info="particle information">
      <column name="status"  type="int8"   num="1"   info="status of the particle"/>
      <column name="charge"  type="int8"   num="2"   info="charge of the particle"/>
      <column name="pid"     type="int16"   num="3"   info="particle ID"/>
      <column name="chi2pid" type="float32" num="4"   info="chi2 for assigned particle id (0.0-1.0)"/>
      <column name="mass"    type="float32" num="5"   info="particle mass calculated from beta"/>
      <column name="beta"    type="float32" num="6"   info="beta of the particle"/>
      <column name="px"      type="float32" num="7"   info="x component of the momentum"/>
      <column name="py"      type="float32" num="8"   info="y component of the momentum"/>
      <column name="pz"      type="float32" num="9"   info="z component of the momentum"/>
      <column name="vx"      type="float32" num="10"  info="x coordinate of vertex"/>
      <column name="vy"      type="float32" num="11"  info="y coordinate of vertex"/>
      <column name="vz"      type="float32" num="12"  info="z coordinate of vertex"/>
    </section>
    <section name="detector"     tag="6002"      num="0"   info="detector responses">
      <column name="pindex"      type="int16"    num="1"   info="index to particle section"/>
      <column name="dindex"      type="int8"     num="2"   info="index to detector banks row"/>
      <column name="detector"    type="int8"     num="3"   info="detector id"/>
      <column name="sector"      type="int8"     num="4"   info="sector number"/>
      <column name="superlayer"  type="int8"     num="5"   info="superlayer number"/>
      <column name="layer"       type="int8"     num="6"   info="layer number"/>
      <column name="component"   type="int8"     num="7"   info="layer number"/>
      <column name="X"           type="float32"  num="8"   info="x coordinate of track intersection"/>
      <column name="Y"           type="float32"  num="9"   info="y coordinate of track intersection"/>
      <column name="Z"           type="float32"  num="10"  info="z coordinate of track intersection"/>
      <column name="hX"          type="float32"  num="11"  info="x coordinate of matched hit"/>
      <column name="hY"          type="float32"  num="12"  info="y coordinate of matched hit"/>
      <column name="hZ"          type="float32"  num="13"  info="z coordinate of matched hit"/>
      <column name="path"        type="float32"  num="14"  info="path to intersection point"/>
      <column name="time"        type="float32"  num="15"  info="time to intersection point"/>
      <column name="energy"      type="float32"  num="16"  info="energy to intersection point"/>
    </section>
    <section name="track"     tag="6003"      num="0"   info="track error matrix">
      
    </section>
  </bank>
</evio_dictionary>
