<evio_dictionary>
  <bank name="RECEVENT" tag="4500" info="EVNT bank for reconstructed CLAS12 Data">
    <section name="particle"  tag="4501" num="0" info="Container Bank">
      <column name="status"   type="int8"    num="1"   info="status of the particle"/>
      <column name="charge"   type="int8"    num="2"   info="charge of the particle"/>
      <column name="pid"      type="int32"   num="3"   info="particle ID"/>
      <column name="mass"     type="float32" num="4"   info="particle mass calculated from beta"/>
      <column name="beta"     type="float32" num="5"   info="x component of momentum"/>
      <column name="scidx"    type="int32"   num="6"   info="particle ID"/>
      <column name="pcalidx"  type="int32"   num="7"   info="particle ID"/>
      <column name="ecinidx"  type="int32"   num="8"   info="particle ID"/>
      <column name="ecoutidx" type="int32"   num="9"   info="particle ID"/>
      <column name="vx"       type="float32" num="10"  info="x component of vertex"/>
      <column name="vy"       type="float32" num="11"  info="y component of vertex"/>
      <column name="vz"       type="float32" num="12"  info="z component of vertex"/>
      <column name="px"       type="float32" num="13"  info="z component of momentum"/>
      <column name="py"       type="float32" num="14"  info="z component of momentum"/>
      <column name="pz"       type="float32" num="15"  info="z component of momentum"/>
      <column name="scpath"   type="float32" num="16"  info="y component of momentum"/>
      <column name="sctime"   type="float32" num="17"  info="y component of momentum"/>
      <column name="scX"      type="float32" num="18"  info="y component of momentum"/>
      <column name="scY"      type="float32" num="19"  info="y component of momentum"/>
      <column name="scZ"      type="float32" num="20"  info="y component of momentum"/>
      <column name="chi2sc"   type="float32" num="21"  info="y component of momentum"/>
      <column name="pcalX"    type="float32" num="22"  info="y component of momentum"/>
      <column name="pcalY"    type="float32" num="23"  info="y component of momentum"/>
      <column name="pcalZ"    type="float32" num="24"  info="y component of momentum"/>
      <column name="chi2pcal" type="float32" num="25"  info="y component of momentum"/>
      <column name="ecinX"    type="float32" num="26"  info="y component of momentum"/>
      <column name="ecinY"    type="float32" num="27"  info="y component of momentum"/>
      <column name="ecinZ"    type="float32" num="28"  info="y component of momentum"/>
      <column name="chi2ecin" type="float32" num="29"  info="y component of momentum"/>
      <column name="ecoutX"   type="float32" num="30"  info="y component of momentum"/>
      <column name="ecoutY"   type="float32" num="31"  info="y component of momentum"/>
      <column name="ecoutZ"   type="float32" num="32"  info="y component of momentum"/>
      <column name="chi2ecout"  type="float32" num="33"  info="y component of momentum"/>
    </section>
  </bank>

  <bank name="EVENTHB" tag="4600" info="HIT based event reconstruction bank">
    <section name="particle" tag="4601"     num="0"   info="particle information">
      <column name="status"  type="int32"   num="1"   info="status of the particle"/>
      <column name="charge"  type="int32"   num="2"   info="charge of the particle"/>
      <column name="pid"     type="int32"   num="3"   info="particle ID"/>
      <column name="chi2pid" type="float32" num="4"   info="chi2 for assigned particle id (0.0-1.0)"/>
      <column name="mass"    type="float32" num="5"   info="particle mass calculated from beta"/>
      <column name="beta"    type="float32" num="6"   info="beta of the particle"/>
      <column name="px"      type="float32" num="7"   info="x component of the momentum"/>
      <column name="py"      type="float32" num="8"   info="y component of the momentum"/>
      <column name="pz"      type="float32" num="9"   info="z component of the momentum"/>
      <column name="vx"      type="float32" num="10"  info="x coordinate of vertex"/>
      <column name="vy"      type="float32" num="11"  info="y coordinate of vertex"/>
      <column name="vz"      type="float32" num="12"  info="z coordinate of vertex"/>
    </section>
    <section name="detector"     tag="4602"     num="0"   info="detector responses">
      <column name="pindex"      type="int32"   num="1"   info="index to particle section"/>
      <column name="index"       type="int32"   num="2"   info="index to detector banks row"/>
      <column name="detector"    type="int32"   num="3"   info="detector id"/>
      <column name="sector"      type="int32"   num="4"   info="sector number"/>
      <column name="superlayer"  type="int32"   num="5"   info="superlayer number"/>
      <column name="layer"       type="int32"   num="6"   info="layer number"/>
      <column name="X"           type="float32" num="7"   info="x coordinate of track intersection"/>
      <column name="Y"           type="float32" num="8"   info="y coordinate of track intersection"/>
      <column name="Z"           type="float32" num="9"   info="z coordinate of track intersection"/>
      <column name="hX"          type="float32" num="10"  info="x coordinate of matched hit"/>
      <column name="hY"          type="float32" num="11"  info="y coordinate of matched hit"/>
      <column name="hZ"          type="float32" num="12"  info="z coordinate of matched hit"/>
      <column name="path"        type="float32" num="13"  info="path to intersection point"/>
      <column name="time"        type="float32" num="14"  info="time to intersection point"/>
      <column name="energy"      type="float32" num="15"  info="energy to intersection point"/>
    </section>
  </bank>

  <bank name="EVENTTB" tag="4700" info="TIME based event reconstruction bank">
    <section name="particle" tag="4701"     num="0"   info="particle information">
      <column name="status"  type="int32"   num="1"   info="status of the particle"/>
      <column name="charge"  type="int32"   num="2"   info="charge of the particle"/>
      <column name="pid"     type="int32"   num="3"   info="particle ID"/>
      <column name="chi2pid" type="float32" num="4"   info="chi2 for assigned particle id (0.0-1.0)"/>
      <column name="mass"    type="float32" num="5"   info="particle mass calculated from beta"/>
      <column name="beta"    type="float32" num="6"   info="beta of the particle"/>
      <column name="px"      type="float32" num="7"   info="x component of the momentum"/>
      <column name="py"      type="float32" num="8"   info="y component of the momentum"/>
      <column name="pz"      type="float32" num="9"   info="z component of the momentum"/>
      <column name="vx"      type="float32" num="10"  info="x coordinate of vertex"/>
      <column name="vy"      type="float32" num="11"  info="y coordinate of vertex"/>
      <column name="vz"      type="float32" num="12"  info="z coordinate of vertex"/>
    </section>
    <section name="detector"     tag="4702"     num="0"   info="detector responses">
      <column name="pindex"      type="int32"   num="1"   info="index to particle section"/>
      <column name="index"       type="int32"   num="2"   info="index to detector banks row"/>
      <column name="detector"    type="int32"   num="3"   info="detector id"/>
      <column name="sector"      type="int32"   num="4"   info="sector number"/>
      <column name="superlayer"  type="int32"   num="5"   info="superlayer number"/>
      <column name="layer"       type="int32"   num="6"   info="layer number"/>
      <column name="X"           type="float32" num="7"   info="x coordinate of track intersection"/>
      <column name="Y"           type="float32" num="8"   info="y coordinate of track intersection"/>
      <column name="Z"           type="float32" num="9"   info="z coordinate of track intersection"/>
      <column name="hX"          type="float32" num="10"  info="x coordinate of matched hit"/>
      <column name="hY"          type="float32" num="11"  info="y coordinate of matched hit"/>
      <column name="hZ"          type="float32" num="12"  info="z coordinate of matched hit"/>
      <column name="path"        type="float32" num="13"  info="path to intersection point"/>
      <column name="time"        type="float32" num="14"  info="time to intersection point"/>
      <column name="energy"      type="float32" num="15"  info="energy to intersection point"/>
    </section>
  </bank>


</evio_dictionary>
