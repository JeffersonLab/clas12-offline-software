<evio_dictionary>
  <bank name="SEBDebug" tag="3010" info="Debug information from SEB swimmer">
    <section name="dc" tag="3011" info="Information about the hits in each layer of DC">
      <column name="index"  num="0"  type="int32"    info="index to generated particle"/>
      <column name="sector" num="1"  type="int32"    info="layer number in DC"/>
      <column name="layer"  num="2"  type="int32"    info="layer number in DC"/>
      <column name="X"      num="3"  type="float64"  info="x-component of the hit"/>
      <column name="Y"      num="4"  type="float64"  info="y-component of the hit"/>
      <column name="Z"      num="5"  type="float64"  info="z-component of the hit"/>
      <column name="uX"     num="6"  type="float64"  info="direction x-component of the hit"/>
      <column name="uY"     num="7"  type="float64"  info="direction y-component of the hit"/>
      <column name="uZ"     num="8"  type="float64"  info="direction z-component of the hit"/>
    </section>

    <section name="ftof" tag="3012" info="Hits in each layer of TOF">
      <column name="index"   num="0"  type="int32"    info="index to generated particle"/>
      <column name="sector"  num="1"  type="int32"    info="sector number"/>
      <column name="layer"   num="2"  type="int32"    info="layer number"/>
      <column name="X"       num="3"  type="float32"  info="x-component of the hit"/>
      <column name="Y"       num="4"  type="float32"  info="y-component of the hit"/>
      <column name="Z"       num="5"  type="float32"  info="z-component of the hit"/>
      <column name="uX"      num="6"  type="float32"  info="direction x-component of the hit"/>
      <column name="uY"      num="7"  type="float32"  info="direction x-component of the hit"/>
      <column name="uZ"      num="8"  type="float32"  info="direction x-component of the hit"/>
    </section>
  </bank>
</evio_dictionary>
