<bank name="ECRec" tag="1520" info="EC Cluster Information Information">
  <section name="hits" tag="1521" num="0" info="Reconstructed hits">
    <column name = "sector"      num = "1"  type = "int32"   info = "sector number"/>
    <column name = "superlayer"  num = "2"  type = "int32"   info = "sector number"/>
