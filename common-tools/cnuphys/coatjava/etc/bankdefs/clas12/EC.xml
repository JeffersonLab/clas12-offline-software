    <column name = "widthV"      num = "7"  type = "float32" info = "width in V layer"/>
    <column name = "widthW"      num = "8"  type = "float32" info = "width in W layer"/>
    <column name = "X"           num = "9"  type = "float32" info = "x coordinate"/>
    <column name = "Y"           num = "10" type = "float32" info = "y coordinate"/>
    <column name = "Z"           num = "12" type = "float32" info = "z coordinate"/>
    <column name = "dX"          num = "13" type = "float32" info = "x coordinate error"/>
    <column name = "dY"          num = "14" type = "float32" info = "y coordinate error"/>
    <column name = "dZ"          num = "15" type = "float32" info = "z coordinate error"/>
  </section>
