  </section>
  <section name="peaks" tag="1522" num="0" info="Reconstructed hits">
    <column name = "sector"      num = "1"  type = "int32"   info = "sector number (1..6)"/>
    <column name = "superlayer"  num = "2"  type = "int32"   info = "PCAL=0, EC inner=1, EC outter=2"/>
    <column name = "view"        num = "3"  type = "int32"   info = "U=0,V=1,W=2"/>
    <column name = "energy"      num = "4"  type = "float64" info = "Peak Energy"/>
